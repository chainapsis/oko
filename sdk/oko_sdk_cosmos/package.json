{
  "name": "@oko-wallet/oko-sdk-cosmos",
  "version": "0.0.6-rc.146",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "del-cli dist",
    "build": "yarn rollup-build && yarn build-declaration",
    "build-declaration": "tsc --build --force --emitDeclarationOnly && tsc-alias",
    "rollup-build": "yarn clean && tsc && rollup -c",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "keplr",
    "oko",
    "cosmos",
    "blockchain",
    "wallet",
    "cosmjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chainapsis/oko.git",
    "directory": "sdk/oko_sdk_cosmos"
  },
  "dependencies": {
    "@cosmjs/amino": "^0.33.1",
    "@cosmjs/proto-signing": "^0.33.1",
    "@keplr-wallet/proto-types": "^0.12.253",
    "@keplr-wallet/types": "^0.12.253",
    "@noble/curves": "1.9.7",
    "@noble/hashes": "^1.8.0",
    "@oko-wallet/oko-sdk-core": "^0.0.6-rc.121",
    "@oko-wallet/stdlib-js": "^0.0.2-rc.39",
    "bech32": "^2.0.0",
    "buffer": "^6.0.3",
    "uuid": "*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "del-cli": "^6.0.0",
    "jest": "^29.7.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "ts-jest": "^29.4.5",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  },
  "gitHead": "0960908938cd494a3b11af557553dba8fb53b674"
