{
  "name": "@oko-wallet/oko-interchain-kit",
  "version": "0.0.1-rc.1",
  "description": "interchain-kit wallet connector for Oko Wallet",
  "author": "oko-wallet",
  "homepage": "https://github.com/hyperweb-io/interchain-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "types": "./esm/index.d.ts",
      "require": "./cjs/index.js"
    }
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "cjs",
    "esm",
    "!CHANGELOG.md",
    "!LICENSE"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json --outDir cjs --module commonjs || true",
    "build:esm": "tsc -p tsconfig.json --outDir esm --module es2022 || true",
    "clean:cjs": "rimraf cjs",
    "clean:esm": "rimraf esm",
    "clean": "npm run clean:cjs && npm run clean:esm",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "prepare": "npm run build",
    "lint": "eslint --ext .tsx,.ts .",
    "format": "eslint --ext .tsx,.ts --fix ."
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "interchain-kit",
    "cosmos",
    "oko",
    "wallet",
    "embed"
  ],
  "dependencies": {
    "@interchain-kit/core": "^0.3.55",
    "@keplr-wallet/types": "^0.12.290",
    "@oko-wallet/oko-sdk-core": "^0.0.6-rc.124",
    "@oko-wallet/oko-sdk-cosmos": "^0.0.6-rc.150",
    "@oko-wallet/oko-sdk-eth": "^0.0.6-rc.139"
  },
  "peerDependencies": {
    "@cosmjs/amino": ">= 0.28",
    "@cosmjs/proto-signing": ">= 0.28"
  }
}
