{
  "name": "@oko-wallet/oko-cosmos-kit",
  "version": "0.0.1-rc.15",
  "type": "module",
  "description": "cosmos-kit wallet connector for Oko Wallet",
  "author": "oko-wallet",
  "homepage": "https://github.com/hyperweb-io/cosmos-kit#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "!CHANGELOG.md",
    "!LICENSE"
  ],
  "scripts": {
    "clean": "del-cli dist",
    "build": "yarn clean && rollup -c --configPlugin typescript && tsc-alias",
