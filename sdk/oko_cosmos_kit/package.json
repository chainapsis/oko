    "@oko-wallet/oko-sdk-cosmos": "^0.0.6-rc.152"
  },
  "peerDependencies": {
    "@cosmjs/amino": ">= 0.28",
    "@cosmjs/proto-signing": ">= 0.28"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "del-cli": "^6.0.0",
    "rollup": "^4.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3"
  },
  "gitHead": "b1c4753a3a1aa996a2f5c28297d71adbe2457945"
