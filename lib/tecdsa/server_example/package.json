    "i": "^0.3.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "npm": "^11.2.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
